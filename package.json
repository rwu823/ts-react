{
  "name": "ts-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@ts-mono/base": "rwu823/ts-mono#base/latest",
    "@types/hard-source-webpack-plugin": "1.0.0",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/react-hot-loader": "4.1.0",
    "@types/styled-components": "4.1.14",
    "@types/webpack": "4.4.29",
    "@types/webpack-dev-server": "3.1.5",
    "add-asset-html-webpack-plugin": "3.1.3",
    "awesome-typescript-loader": "5.2.1",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "3.2.0",
    "npm-run-all": "4.1.5",
    "typescript-plugin-styled-components": "1.4.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "3.3.1"
  },
  "scripts": {
    "transcrypt": "curl -s https://raw.githubusercontent.com/elasticdog/transcrypt/master/transcrypt | bash -s --",
    "build:dll": "webpack --config webpack.dll.ts",
    "build:prod": "webpack -p",
    "dev": "run-s build:dll && webpack-dev-server --hot",
    "build": "rm -rf out && NODE_ENV=production run-s build:dll build:prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.8.4",
    "styled-components": "4.2.0"
  }
}
