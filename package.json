{
  "name": "ts-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@ts-mono/base": "rwu823/ts-mono#base/latest",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.14",
    "@zeit/next-typescript": "1.1.1",
    "babel-plugin-styled-components": "1.10.0",
    "npm-run-all": "4.1.5"
  },
  "scripts": {
    "transcrypt": "curl -s https://raw.githubusercontent.com/elasticdog/transcrypt/master/transcrypt | bash -s --",
    "build:dll": "webpack --config webpack.dll.ts",
    "build:prod": "webpack -p",
    "dev": "next src",
    "export": "rm -rf out && next build src && next export src && mv src/out out",
    "build": "rm -rf out && NODE_ENV=production run-s build:dll build:prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "next": "8.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.2.0"
  }
}
